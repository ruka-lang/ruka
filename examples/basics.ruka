// Type definition
local Dog = record {
    name: string,
    breed: string
}

// Static function definition
const new (Dog) = (name, breed) do
    { name, breed }
end

// Method definition
const about (self: Dog) = () do
    string.new("My name's ${self.name}. I'm a ${self.breed}")
end

const main = () do
    local ruka = Dog.new("Ruka", "Jack Russel Terrier")
    ruka.about()
        |> toUpperCase()
        |> @println()
end
