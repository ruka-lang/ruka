local Dog = record {
    name: string,
    breed: string
}

const new () = (name, breed) do
    { name, breed }
end

const about (self: Dog) = () do
    "My name's ${self.name}. I'm a ${self.breed}"
end

const main = () do
    local ruka = Dog.new("Ruka", "Jack Russel Terrier")
    ruka
        |> about()
        |> toUpperCase()
        |> @println()
end
