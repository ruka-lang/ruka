local std = @package("std")
local { animal } = @import("otherStuff")

const main = () do
    let dog = animal {
        species = dog("Ruka"),
        age = 5
    }

    dog.method(12)
    @assertEq(dog.age == 12).?

    let name = match dog.species {
        dog(name) => name,
        cat(name) => name,
        bird => "Gustave"
    }

    @print("#{name}'s species is #{dog.species} and their age is #{dog.age}\n")
end
