local std = @package("std")
local otherStuff = @import("otherStuff")
local {message, animal, ..} = otherStuff

const main = () do
    let dog = {
        species = dog("Ruka"),
        age = 5
    }

    let name = match dog.species do
        dog(name) => name,
        cat(name) => name,
        bird => "Gustave"
    end

    @print("#{name}'s species is #{dog.species} and their age is #{dog.age}\n")
end
